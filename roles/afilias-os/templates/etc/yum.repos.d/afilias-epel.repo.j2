[epel]
name=Extra Packages for Enterprise Linux {{ ansible_distribution_major_version }}
baseurl={{ yum_base_url }}/epel/{{ ansible_distribution_major_version }}/{{ ansible_architecture }}/
failovermethod=priority
enabled=1
gpgcheck=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-EPEL-{{ ansible_distribution_major_version }}


